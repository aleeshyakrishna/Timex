								if (response.status) {

									location.replace('/my-account/orders')

								} else {

									Swal.fire({

										icon: 'error',

										title: 'Oops...',

										text: 'Something went wrong!',

										footer: '<a href="/">Goto home page</a>'

									})

								}

							}

						})

					})

				},

			})

			.render("#paypal1")

	}




