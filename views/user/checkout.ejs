			flag = 2

		} else if (stateValue.match(/^[a-zA-Z]+(([',. -][a-zA-Z ])?[a-zA-Z]*)*$/)) {

			error6.innerText = 'Enter valid details'

			flag = 2

		}

		//Phone Number

